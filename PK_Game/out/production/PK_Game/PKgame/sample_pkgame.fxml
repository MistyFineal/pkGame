<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PKgame.Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="message" layoutX="207.0" layoutY="316.0" prefHeight="36.0" prefWidth="371.0" text="接続するサーバのIPアドレスを入力し、ゲーム開始ボタンを押してください" textAlignment="CENTER" />
            <Label layoutX="174.0" layoutY="14.0" prefHeight="110.0" prefWidth="323.0" text="PK Game" textAlignment="CENTER">
               <font>
                  <Font name="Georgia" size="71.0" />
               </font>
            </Label>
            <Button fx:id="ok_Button" layoutX="302.0" layoutY="276.0" mnemonicParsing="false" onAction="#ok_Click" text="ゲーム開始" />
            <TextField fx:id="text_Field" layoutX="261.0" layoutY="361.0" />
            <ListView fx:id="listView" layoutX="2.0" layoutY="267.0" prefHeight="134.0" prefWidth="205.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
